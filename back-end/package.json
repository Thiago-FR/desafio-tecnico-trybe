{
  "name": "solid-1-exercises",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npx tsc",
    "db:reset": "npx prisma migrate dev --name create-table",
    "prestart": "npm run build && npm run db:reset",
    "start": "node ./dist/",
    "dev": "ts-node-dev index.ts",
    "coverage": "nyc npm run test",
    "test": "mocha -r ts-node/register ./src/tests/*$NAME*.{test,spec}.ts -t 30000 --exit",
    "docker:up": "docker-compose up -d --build",
    "docker:down": "docker-compose down --remove-orphans"
  },
  "keywords": [
    "typescript",
    "solid",
    "refactor"
  ],
  "author": "Pessoa Estudante",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "4.3.0",
    "@types/chai-http": "4.2.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.7.8",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "mocha": "9.2.1",
    "nodemon": "^2.0.16",
    "nyc": "^15.1.0",
    "prisma": "^3.14.0",
    "sinon": "13.0.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "eslint-config-trybe-backend": "^1.0.4",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "ts-node-dev": "^1.1.8"
  }
}
